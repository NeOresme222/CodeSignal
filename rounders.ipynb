   "source": [
    "def rounders(n):\n",
    "    strn = str(n)\n",
    "    bwdstr = ''\n",
    "    total = 0\n",
    "    for i in strn[:0:-1]:\n",
    "        if int(i) + total >= 5:\n",
    "            total = 1\n",
    "        else:\n",
    "            total = 0\n",
    "        bwdstr += '0'\n",
    "\n",
    "    return int(str(int(strn[0]) + total) + bwdstr[::-1])\n"
   ]
  }
