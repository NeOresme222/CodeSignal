    "def pagesNumberingWithInk(current, numberOfDigits):\n",
    "    nd = numberOfDigits\n",
    "    while nd > 0:\n",
    "        page = len(str(current))\n",
    "        if nd >= page:\n",
    "            nd -= page\n",
    "            current += 1\n",
    "        if nd < page:\n",
    "            return current - 1"
   ]
  }
