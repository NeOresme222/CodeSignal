    "def isTandemRepeat(inputString):\n",
    "    if len(inputString) % 2 == 0:\n",
    "        half = int(len(inputString)/2)\n",
    "        if inputString[:half] == inputString[half:]:\n",
    "            return True\n",
    "    return False"
   ]
