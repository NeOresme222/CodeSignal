    "def replaceMiddle(arr):\n",
    "    \n",
    "    if len(arr) % 2 == 0:\n",
    "        mid = int(len(arr) / 2)\n",
    "        middle = (arr[mid - 1] + arr[mid])\n",
    "        del arr[mid]\n",
    "        \n",
    "        return [middle if i == mid - 1 else arr[i] for i in range(len(arr))]\n",
    "    \n",
    "    else:\n",
    "        return arr"
   ]
