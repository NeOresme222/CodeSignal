    "def countBlackCells(n, m):\n",
    "    gcd = n\n",
    "    b = m\n",
    "\n",
    "    while b != 0:\n",
    "        gcd, b = b, gcd % b\n",
    "\n",
    "        \n",
    "    line_cells = n + m - gcd\n",
    "    line_corner_cells = (gcd - 1) * 2\n",
    "    return line_cells + line_corner_cells"
   ]
  }
 ],
