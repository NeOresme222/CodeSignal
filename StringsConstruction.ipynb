    "def stringsConstruction(a, b):\n",
    "    d = dict()\n",
    "    \n",
    "    for letter in a:\n",
    "        if letter not in d:\n",
    "            A = a.count(letter)\n",
    "            B = b.count(letter)\n",
    "            d[letter] = (B // A)\n",
    "        \n",
    "    return min(d.values())\n"
   ]
  }
